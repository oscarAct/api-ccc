{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "no-unused-vars": "warn",
        "no-console": "off",
        "node-process-exit": "off",
        "func-names": "off",
        "prefer-const": "warn",
        "object-shorthand": "warn",
        "camelcase": "warn",
        "no-var": "warn",
        "vars-on-top": "warn",
        "consistent-return": "off",
        "block-scoped-var": "warn"
    }
}
